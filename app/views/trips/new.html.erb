<div class="topnav" id="myTopnav">
  <%= link_to "Home", users_path %>
  <%= link_to "All Trips", user_trips_path(current_user) %>
  <%= link_to "Logout", logout_path, class: "logout" %>
  <%= link_to "Profile", user_path(current_user), class: "logout" %>
</div>

<%= form_for @trip, url: user_trips_path, action: 'post' do |f| %>

  <%= f.hidden_field :user_id, value: current_user.id %>

  <%= render partial: 'form', locals: { f: f } %>

<% end %>
